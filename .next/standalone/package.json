{
  "name": "doganconsult-gaming-paradise",
  "version": "2.0.0",
  "description": "DoganConsult - Unified Business + AI Consultation Site with Gaming Paradise Features",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:test": "tsc --noEmit --project tsconfig.test.json",
    "test": "jest --coverage --watchAll",
    "test:unit": "jest --testPathPattern=__tests__/unit --coverage",
    "test:integration": "jest --testPathPattern=__tests__/integration --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:performance": "jest --testPathPattern=__tests__/performance --coverage",
    "test:api": "jest --testPathPattern=__tests__/api --coverage",
    "test:components": "jest --testPathPattern=__tests__/components --coverage",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx,ts,tsx}' --coverageReporters=text --coverageReporters=html --coverageReporters=lcov",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "docker:build": "docker build -t doganconsult-gaming .",
    "docker:run": "docker run -p 3002:3002 doganconsult-gaming",
    "deploy": "npm run build && npm run start",
    "lighthouse": "lighthouse http://localhost:3002 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:ci": "lighthouse http://localhost:3002 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'"
  },
  "keywords": [
    "doganconsult",
    "gaming-paradise",
    "ai-agents",
    "business-consultation",
    "strategy-arena",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "Ahmet Dogan",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "express-rate-limit": "^7.1.0",
    "framer-motion": "^10.16.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.263.1",
    "mongodb": "^6.0.0",
    "next": "^14.0.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "cypress": "^13.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.7.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doganconsult/gaming-paradise"
  },
  "bugs": {
    "url": "https://github.com/doganconsult/gaming-paradise/issues"
  },
  "homepage": "https://www.doganconsult.com"
}
